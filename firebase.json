{
  "hosting": {
    "public": "frontend/dist/test",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "functions": [
    {
      "source": "backend/test",
      "runtime": "nodejs18",
      "codebase": "default"
    }
  ]
}
